{
  "extends": [
    "../../node_modules/react-scripts/node_modules/eslint-config-react-app",
    "prettier",
    "eslint:recommended"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.ts?(x)"
      ],
      "rules": {
      }
    }
  ]
}
